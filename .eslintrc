{
  "extends": ["airbnb-base"],
  "env": {
    "jest": true,
    "node": true
  },
  "rules": {
    "no-use-before-define": 0,
    "import/prefer-default-export": 0,
    "import/no-unresolved": 0,
    "no-underscore-dangle": ["error", { "allowAfterThis": true }],
    "lines-between-class-members": [
      "error",
      "always",
      { "exceptAfterSingleLine": true }
    ]
  }
}
